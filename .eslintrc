{
    "extends": [
        "eslint:recommended"
    ],
    "rules": {
        "semi": "error",
        "no-unused-vars": 0,
        "angular/no-services": 0,
        "no-console": 0
    },
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "globals": {
        "THREE": true,
        "Stats": true,
        "io": true
    }
}